{
  "compileOnSave": false,
  "compilerOptions": {
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "classic",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strictNullChecks": true,
    "strict": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "target": "es5",
    "typeRoots": [
    ],
    "lib": [
      "es2017",
      "dom"
    ]
  }
}
